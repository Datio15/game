#include <iostream>
using namespace std;
int main(){
  int mum1,num2;
  char op;
  cin >> num1 >> op >> num2;
  swich(op){
    case '+' :
      cout << num1 + num2 << endl;
    case '-' :
      cout << num1 - num2 << endl;
case '*':
cout << num1 * num2 <<endl;
case '/':
cout << num1 /(num2*1.0) <<endl;
case '%' :
cout << num1 % num2 << endl;
default:
cout << "Invalid Operator" << endl;
  }
}
